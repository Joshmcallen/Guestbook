extends layout

block content
  h1 test userlist.jade


  table
    thead
      td  Name
      td  Email
      td  Message
      td
    tbody
      each user, i in userlist
        tr
          td
            a(href="/view/#{user.username}")= user.username
          td
            =user.email
          td
            =user.message
          td
            a(href="/#{user._id}")
              button.btn.btn-danger(type='button') DELETE
